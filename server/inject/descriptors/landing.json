{
  "type": "object",
  "properties": {
    "islanding": {
      "type": "identifier",
      "selector": ".app-name.block.ng-binding",
      "control": "exists",
      "selectContains": "Homepages"
    },
    "clickhambager": {
      "type": "action",
      "selector": ".displayBlock.m-menu-btn.ng-scope",
      "control": "action.link"
    },
    "clicksn1t": {
      "type": "action",
      "selector": "body",
      "control": "action.link"
    },
    "ismenuopen": {
      "type": "identifier",
      "selector": "body",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "islanding"
      ],
      "action": "openMenu"
    },
    {
      "match": [],
      "action": "clicksn1t"
    }
  ]
}
