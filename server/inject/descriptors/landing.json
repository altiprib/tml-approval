{
  "type": "object",
  "properties": {
    "islanding": {
      "type": "identifier",
      "selector": ".app-name.block.ng-binding",
      "control": "exists",
      "selectContains": "Homepages"
    },
    "clickhambager": {
      "type": "action",
      "selector": ".displayBlock.m-menu-btn.ng-scope",
      "control": "action.link"
    },
    "ismenuopen": {
      "type": "identifier",
      "selector": ".icon-block-wrapper",
      "control": "exists"
    },
    "clicksn1t": {
      "type": "action",
      "selector": "[title=\"SN1T\"]",
      "control": "action.button"
    },
    "isrecentlyused": {
      "type": "identifier",
      "control": "exists",
      "selector": "label#node-options-label"
    }
  },
  "connect": [
    {
      "match": [
        "islanding"
      ],
      "action": "openMenu",
      "model": "landing"
    },
    {
      "match": [
        "islanding",
        "ismenuopen"
      ],
      "action": "clicksn1t",
      "model": "landing"
    }
  ]
}
